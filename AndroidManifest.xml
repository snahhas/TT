<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.taxtimereset">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GasTrackerMain"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait" />
        <activity
            android:name=".restaurant"
            android:screenOrientation="portrait" />
        <activity
            android:name=".supplies"
            android:screenOrientation="portrait" />
        <activity
            android:name=".rent"
            android:screenOrientation="portrait" />
        <activity
            android:name=".maintanance"
            android:screenOrientation="portrait" />
        <activity
            android:name=".utilities"
            android:screenOrientation="portrait" />
        <activity
            android:name=".inventory"
            android:screenOrientation="portrait" />
        <activity
            android:name=".marketing"
            android:screenOrientation="portrait" />
        <activity android:name=".gas_tracker_summary" />
        <activity android:name=".history_view"></activity>
    </application>

</manifest>